{
  "name": "dev-env-installer",
  "version": "1.0.0",
  "description": "macOS Development Environment One-Click Installer",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
    "build:check": "vue-tsc && vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"npm run dev\" \"wait-on http://localhost:5173 && electron .\""
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.0",
    "element-plus": "^2.4.0",
    "glob": "^13.0.0",
    "node-pty": "^1.1.0",
    "pinia": "^2.1.0",
    "tree-kill": "^1.2.2",
    "vue": "^3.4.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.0",
    "fast-check": "^4.5.2",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.0",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1",
    "wait-on": "^7.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "esbuild",
      "node-pty"
    ]
  }
}